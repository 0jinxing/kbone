<!-- 视图容器 -->
<template name="cover-image">
    <cover-image
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"
        
        src="{{src}}"
        bindload="onCoverImageLoad"
        binderror="onCoverImageError"
    ></cover-image>
</template>
<!-- 基础内容 -->
<template name="icon">
    <icon
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"

        type="{{type}}"
        size="{{size}}"
        color="{{color}}"
    ></icon>
</template>
<template name="progress">
    <progress
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"

        percent="{{percent}}"
        show-info="{{showInfo}}"
        border-radius="{{borderRadius}}"
        font-size="{{fontSize}}"
        stroke-width="{{strokeWidth}}"
        color="{{color}}"
        active-color="{{activeColor}}"
        background-color="{{backgroundColor}}"
        active="{{active}}"
        active-mode="{{activeMode}}"
        bindactiveend="onProgressActiveEnd"
    ></progress>
</template>
<template name="rich-text">
    <rich-text
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"

        nodes="{{nodes}}"
        space="{{space}}"
    ></rich-text>
</template>
<!-- 表单组件 -->
<template name="editor">
    <editor
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"

        read-only="{{readOnly}}"
        placeholder="{{placeholder}}"
        show-img-size="{{showImgSize}}"
        show-img-toolbar="{{showImgToolbar}}"
        show-img-resize="{{showImgResize}}"
        bindready="onEditorReady"
        bindfocus="onEditorFocus"
        bindblur="onEditorBlur"
        bindinput="onEditorInput"
        bindstatuschange="onEditorStatusChange"
    ></editor>
</template>
<template name="input">
    <radio-group
        wx:if="{{type === 'radio'}}"
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindchange="onRadioChange"
    ><radio
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"

        value="{{value}}"
        disabled="{{disabled}}"
        checked="{{checked}}"
        color="{{color}}"
    ></radio></radio-group>
    <checkbox-group
        wx:elif="{{type === 'checkbox'}}"
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindchange="onCheckboxChange"
    ><checkbox
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"

        value="{{value}}"
        disabled="{{disabled}}"
        checked="{{checked}}"
        color="{{color}}"
    ></checkbox></checkbox-group>
    <input
        wx:elif="{{type !== 'hidden'}}"
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"
        
        value="{{value}}"
        type="{{type}}"
        password="{{password}}"
        placeholder="{{placeholder}}"
        placeholder-style="{{placeholderStyle}}"
        placeholder-class="{{placeholderClass}}"
        disabled="{{disabled}}"
        maxlength="{{maxlength}}"
        cursor-spacing="{{cursorSpacing}}"
        auto-focus="{{autoFocus}}"
        focus="{{focus}}"
        confirm-type="{{confirmType}}"
        confirm-hold="{{confirmHold}}"
        cursor="{{cursor}}"
        selection-start="{{selectionStart}}"
        selection-end="{{selectionEnd}}"
        adjust-position="{{adjustPosition}}"
        bindinput="onInputInput"
        bindfocus="onInputFocus"
        bindblur="onInputBlur"
        bindconfirm="onInputConfirm"
        bindkeyboardheightchange="onInputKeyBoardHeightChange"
    ></input>
</template>
<template name="slider">
    <slider
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"

        min="{{min}}"
        max="{{max}}"
        step="{{step}}"
        disabled="{{disabled}}"
        value="{{value}}"
        color="{{color}}"
        selected-color="{{selectedColor}}"
        active-color="{{activeColor}}"
        background-color="{{backgroundColor}}"
        block-size="{{blockSize}}"
        block-color="{{blockColor}}"
        show-value="{{showValue}}"
        bindchange="onSliderChange"
        bindchanging="onSliderChanging"
    ></slider>
</template>
<template name="switch">
    <switch
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"

        checked="{{checked}}"
        disabled="{{disabled}}"
        type="{{type}}"
        color="{{color}}"
        bindchange="onSwitchChange"
    ></switch>
</template>
<template name="textarea">
    <textarea
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
        bindtouchstart="onTouchStart"
        bindtouchmove="onTouchMove"
        bindtouchend="onTouchEnd"
        bindtouchcancel="onTouchCancel"
        bindtap="onTap"

        value="{{value}}"
        placeholder="{{placeholder}}"
        placeholder-style="{{placeholderStyle}}"
        placeholder-class="{{placeholderClass}}"
        disabled="{{disabled}}"
        maxlength="{{maxlength}}"
        auto-focus="{{autoFocus}}"
        focus="{{focus}}"
        auto-height="{{autoHeight}}"
        fixed="{{fixed}}"
        cursor-spacing="{{cursorSpacing}}"
        cursor="{{cursor}}"
        show-confirm-bar="{{showConfirmBar}}"
        selection-start="{{selectionStart}}"
        selection-end="{{selectionEnd}}"
        adjust-position="{{adjustPosition}}"
        bindfocus="onTextareaFocus"
        bindblur="onTextareaBlur"
        bindlinechange="onTextareaLineChange"
        bindinput="onTextareaInput"
        bindconfirm="onTextareaConfirm"
        bindkeyboardheightchange="onTextareaKeyBoardHeightChange"
    ></textarea>
</template>
<!-- 媒体组件 -->
<template name="image">
    <image
        id="{{id}}"
        class="{{class}}"
        style="{{style}}"
        hidden="{{hidden}}"
        data-private-node-id="{{nodeId}}"
        data-private-page-id="{{pageId}}"
