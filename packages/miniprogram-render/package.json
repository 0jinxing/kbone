{
	"name": "miniprogram-render",
	"version": "1.0.2",
	"description": "web's adapter for miniprogram",
	"main": "src/index.js",
	"miniprogram": "src",
	"scripts": {
		"test": "jest --bail",
		"test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --bail",
		"coverage": "jest --coverage --bail",
		"codecov": "jest --coverage && codecov"
	},
	"jest": {
		"testEnvironment": "node",
		"testMatch": [
			"**/test/**/*.test.js"
		],
		"collectCoverageFrom": [
			"src/**/*.js"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/wechat-miniprogram/kbone.git"
	},
	"author": "wechat-miniprogram",
	"license": "MIT",
	"devDependencies": {
		"jest": "^24.8.0",
		"jsdom": "^15.1.1"
	},
	"gitHead": "30c26f3f71ead7a2a50cfec49549011a11fe73af"
}
